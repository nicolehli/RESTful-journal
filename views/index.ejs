<% include partials/header %>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>"><span><%= blog.created %></span>
        <P><%= blog.body.substring(0, 100) %>...</P>
        <a href="/blogs/<%= blog._id %>">READ MORE</a>
    </div>
<% }) %>

<% include partials/footer %>